mixin timetable(type, order, title, name, accounts)
  tr.timetable-row
    td #{order}
    if block
      td
        a.btns(data-tgt="wd-#{type}#{order}") #{title}
        .modal(class="wd-#{type}#{order}")
          .modalBody
            h4 概要
            block
          .modalBK
    else
      td #{title}
    td.name
      | #{name}
      if accounts
        each url, index in accounts
          //- "http://".length == 7
          //- urlが配列（複数ある）か文字列（ひとつだけ）かの区別のしかたがわからなかった…
          if url.length < 7
            each u in url
              +display_icon(index, u)
          else
            +display_icon(index, url)

mixin display_icon(index, url)
  if index == "twitter"
    +twitter_icon(url)
  else
    +web_icon(url)

